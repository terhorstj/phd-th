@article{kang,
	Abstract = {The effect of basis sets on the structures and energetics of the rotation for the peptide bond of $N$-methylacetamide (NMA) has been investigated at high levels of theory. The structures, dipole moments and thermodynamic properties for \textit{trans}- and \textit{cis}-NMA have been determined at the HF, MP2 and B3LYP levels with 6-31G(d), 6-31G(d,p), 6-31+G(d), 6-31+G(d,p), 6-31++G(d,p), 6-311+G(d,p) and 6-311++G(d,p) basis sets. By comparing the calculated structure and dipole moment of \textit{trans}-NMA and the relative enthalpy (ΔH) of \textit{cis}-NMA in the gas phase with corresponding experimental data, the B3LYP level with 6-31G(d) or 6-31G(d,p) basis sets appears to be the most appropriate among several levels of theory and basis sets considered here. \textit{trans}-NMA is computed to be favored by a total free energy of 2.7kcal/mol than \textit{cis}-NMA in water at the B3LYP/6-31G(d,p) level with the SCI-PCM method, which corresponds to 1.1\% population of \textit{cis}-NMA. This is consistent with the results of NMR experiments in aqueous solution that estimate the \textit{cis} population to be 1.5$\pm$0.1\% and the free energy difference of 2.5$\pm$0.3kcal/mol. In addition, the change in relative solvation free energies is computed to be --0.1kcal/mol at the HF and B3LYP levels with the 6-31G(d,p) basis set as transferred from water to chloroform, which corresponds to the increase of the \textit{cis} population by about 0.3\%, consistent with the value from NMR measurements. Therefore, the SCRF method with proper levels of theory and basis sets seems to be reasonable to describe the solvation of NMA. Our calculated results confirmed that the relative distribution of \textit{trans} and \textit{cis} conformers of NMA is insensitive to the solvent polarity.},
	Author = {Young Kee Kang},
	Date-Added = {2019-06-21 15:01:34 -0700},
	Date-Modified = {2019-06-21 15:01:42 -0700},
	Doi = {https://doi.org/10.1016/S0166-1280(01)00445-6},
	Issn = {0166-1280},
	Journal = {The Journal of Molecular Structure: THEOCHEM},
	Keywords = {-methylacetamide, Ab initio calculations, \textit{cis}--\textit{trans} equilibrium, Solvation},
	Number = {1},
	Pages = {183-193},
	Title = {Ab initio MO and density functional studies on \textit{trans} and \textit{cis} conformers of $N$-methylacetamide},
	Url = {http://www.sciencedirect.com/science/article/pii/S0166128001004456},
