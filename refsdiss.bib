	Issn = {0022-2836},
	Journal = {The Journal of Molecular Biology},
	Keywords = {RNA folding, GCAA, implicit solvent, stochastic dynamics, sugar pucker},
	Number = {4},
	Pages = {493-506},
	Title = {RNA simulations: probing hairpin unfolding and the dynamics of a GNRA tetraloop},
