	Issn = {1573-8779},
	Journal = {The Journal of Structural Chemistry},
	Number = {5},
	Pages = {748--755},
	Title = {Theoretical analysis and experimental study of the spatial structure and isomerism of acetone azine and its cyclization to 3,5,5-trimethyl-4,5-dihydro-$1H$-pyrazole},
